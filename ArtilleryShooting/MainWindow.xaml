<Window x:Class="ArtilleryShooting.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ArtilleryShooting"
        mc:Ignorable="d"
        Title="Артелерийская стрельба" Height="600" Width="1000">
    <Grid Name="myGrid">
        <Canvas Name="GameArea" Focusable="True" KeyDown="GameArea_KeyDown" KeyUp="GameArea_KeyUp">
            <Rectangle Name="Pyshka" Width="100" Height="50" Fill="Black" Canvas.Bottom="0" RenderTransformOrigin="0.5, 0.5" Canvas.Top="543" Canvas.Left="-50"/>
            <Rectangle Name="Mishen" Width="50" Height="50" Fill="Black" Canvas.Bottom="400" Canvas.Left="800" RenderTransformOrigin="0.5, 0.5"></Rectangle>
            <Button x:Name="button" Content="Начать игру" Canvas.Left="51" Canvas.Top="46" Width="159" Click="Button_Click" Height="22"/>
            <Label x:Name="label" Content="Нажмите F чтобы стрелять" Canvas.Left="53" Canvas.Top="73"/>
        </Canvas>
    </Grid>
</Window>
